	<kert:test id="1">
		<kert:author>Claudius Teodorescu</kert:author>
		<kert:title>Get text fields</kert:title>
		<kert:version>0.1</kert:version>
		<kert:description>
			Extracts the text fields from a PDF file. The test
			passes if the number of those text fields is as stated.
		</kert:description>
		<kert:test-url>unit-tests/get-text-fields.xq</kert:test-url>
		<kert:source-url>http://127.0.0.1:8080/utils/kert-new/services/source.xq?url=/tests/crypto/unit-tests/get-text-fields.xq
		</kert:source-url>
		<kert:assertion>/result/result-token</kert:assertion>
		<kert:timeout />
		<kert:run-status timestamp="" />
		<kert:development-status timestamp="" />
		<kert:log />
		<kert:loops-number />
		<kert:total-test-time />
		<kert:type>unattended</kert:type>
		<kert:assertion-type>assert-xml</kert:assertion-type>
	</kert:test>
	<kert:test id="2">
		<kert:author>Claudius Teodorescu</kert:author>
		<kert:title>Set text fields</kert:title>
		<kert:version>0.1</kert:version>
		<kert:description>
			Set the text fields of a PDf file. The test is
			passed if the content of the PDF file is updated as needed.
		</kert:description>
		<kert:test-url>unit-tests/set-text-fields.xq</kert:test-url>
		<kert:source-url>http://127.0.0.1:8080/utils/kert-new/services/source.xq?url=/tests/crypto/unit-tests/set-text-fields.xq
